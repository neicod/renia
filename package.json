{
  "name": "frontend-ssr",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run dev:server",
    "dev:server": "NODE_PATH=./modules NODE_ENV=development tsx src/server/index.tsx",
    "dev:client": "node scripts/build-client.mjs --watch",
    "build": "npm run build:client && npm run build:server",
    "build:client": "node scripts/build-client.mjs",
    "build:server": "node scripts/build-server.mjs",
    "start": "NODE_PATH=./modules node dist/server/index.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "express": "^4.18.2",
    "fast-deep-equal": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "renia-module-registry": "file:modules/renia-module-registry",
    "renia-router": "file:modules/renia-router",
    "renia-interceptors": "file:modules/renia-interceptors",
    "renia-layout": "file:modules/renia-layout",
    "renia-magento-cart": "file:modules/renia-magento-cart",
    "magento-wishlist": "file:modules/magento-wishlist"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "esbuild": "^0.19.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
