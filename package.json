{
  "name": "frontend-ssr",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "generate": "npm run generate:interceptors",
    "dev": "npm run generate && npm run dev:server",
    "predev:server": "npm run generate",
    "dev:server": "NODE_ENV=development tsx app/entry/server/index.tsx",
    "dev:client": "npm run generate && node scripts/build-client.mjs --watch",
    "generate:interceptors": "node scripts/generate-interceptor-map.mjs",
    "build": "npm run generate && npm run build:i18n && npm run build:client && npm run build:server",
    "build:docker": "npm run generate && npm run build:i18n && npm run build:client && npm run build:server",
    "build:i18n": "node scripts/build-i18n.mjs",
    "build:client": "node scripts/build-client.mjs",
    "build:server": "node scripts/build-server.mjs",
    "build:packages": "node scripts/build-packages.mjs",
    "start": "node dist/server/index.js",
    "pretest": "npm run generate",
    "test": "tsx --test tests/**/*.test.ts",
    "clean": "rm -rf dist",
    "clean:modules": "find app/modules -type d -name node_modules -prune -exec rm -rf '{}' +",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "fast-deep-equal": "^3.1.3",
    "renia-magento-product": "file:app/modules/renia/magento-product",
    "renia-magento-wishlist": "file:app/modules/renia/magento-wishlist",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "renia-graphql-client": "file:app/modules/renia/graphql-client",
    "renia-i18n": "file:app/modules/renia/i18n",
    "renia-magento-cart": "file:app/modules/renia/magento-cart",
    "renia-magento-cart-sidebar": "file:app/modules/renia/magento-cart-sidebar",
    "renia-magento-catalog": "file:app/modules/renia/magento-catalog",
    "renia-magento-catalog-search": "file:app/modules/renia/magento-catalog-search",
    "renia-magento-category": "file:app/modules/renia/magento-category",
    "renia-magento-cms": "file:app/modules/renia/magento-cms",
    "renia-magento-configurable-product": "file:app/modules/renia/magento-configurable-product",
    "renia-magento-configurable-product-cart": "file:app/modules/renia/magento-configurable-product-cart",
    "renia-magento-customer": "file:app/modules/renia/magento-customer",
    "renia-magento-graphql-client": "file:app/modules/renia/magento-graphql-client",
    "renia-magento-routing": "file:app/modules/renia/magento-routing",
    "renia-magento-store": "file:app/modules/renia/magento-store",
    "renia-magento-product-listing": "file:app/modules/renia/magento-product-listing",
    "renia-module-cart": "file:app/modules/renia/module-cart",
    "renia-ui-toast": "file:app/modules/renia/ui-toast",
    "renia-menu": "file:app/modules/renia/menu",
    "renia-logger": "file:app/modules/renia/logger",
    "@renia/framework": "file:app/modules/renia/framework"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
